{
  "name": "ffmpeg-kit-tvos-video",
  "version": "6.0",
  "summary": "FFmpeg Kit tvOS Video Shared Framework",
  "description": "Includes FFmpeg with dav1d, fontconfig, freetype, fribidi, kvazaar, libass, libtheora, libvpx, snappy, libwebp and zimg libraries enabled.",
  "homepage": "https://github.com/arthenica/ffmpeg-kit",
  "authors": {
    "ARTHENICA": "open-source@arthenica.com"
  },
  "license": {
    "type": "LGPL-3.0",
    "file": "ffmpegkit.xcframework/tvos-arm64/ffmpegkit.framework/LICENSE"
  },
  "platforms": {
    "tvos": "11.0"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/arthenica/ffmpeg-kit/releases/download/v6.0/ffmpeg-kit-video-6.0-tvos-xcframework.zip"
  },
  "tvos": {
    "frameworks": [
      "AudioToolbox",
      "VideoToolbox",
      "CoreMedia"
    ],
    "vendored_frameworks": [
      "ffmpegkit.xcframework",
      "libavcodec.xcframework",
      "libavdevice.xcframework",
      "libavfilter.xcframework",
      "libavformat.xcframework",
      "libavutil.xcframework",
      "libswresample.xcframework",
      "libswscale.xcframework"
    ]
  }
}
