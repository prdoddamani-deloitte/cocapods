{
  "name": "ffmpeg-kit-macos-min-gpl",
  "version": "6.0.LTS",
  "summary": "FFmpeg Kit macOS Min GPL Shared Framework",
  "description": "Includes FFmpeg with libvid.stab, x264, x265 and xvidcore libraries enabled.",
  "homepage": "https://github.com/arthenica/ffmpeg-kit",
  "authors": {
    "ARTHENICA": "open-source@arthenica.com"
  },
  "license": {
    "type": "GPL-3.0",
    "file": "ffmpegkit.framework/Resources/LICENSE"
  },
  "platforms": {
    "osx": "10.12"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/arthenica/ffmpeg-kit/releases/download/v6.0.LTS/ffmpeg-kit-min-gpl-6.0.LTS-macos-framework.zip"
  },
  "osx": {
    "frameworks": [
      "AudioToolbox",
      "CoreAudio",
      "CoreImage",
      "CoreMedia",
      "OpenCL",
      "OpenGL",
      "VideoToolbox"
    ],
    "vendored_frameworks": [
      "ffmpegkit.framework",
      "libavcodec.framework",
      "libavdevice.framework",
      "libavfilter.framework",
      "libavformat.framework",
      "libavutil.framework",
      "libswresample.framework",
      "libswscale.framework"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=macosx*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=macosx*]": "arm64"
  }
}
