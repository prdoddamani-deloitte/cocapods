{
  "name": "BrainCloudCpp",
  "version": "5.0.0",
  "summary": "The C++ client library for brainCloud",
  "homepage": "http://getbraincloud.com/",
  "platforms": {
    "ios": "11.0",
    "osx": "10.13",
    "tvos": "11.0",
    "watchos": "4.0"
  },
  "license": {
    "type": "Apache License, Version 2.0",
    "file": "LICENSE"
  },
  "authors": {
    "brainCloud Team": "support@getbraincloud.com"
  },
  "source": {
    "git": "https://github.com/getbraincloud/braincloud-cpp.git",
    "tag": "5.0.0"
  },
  "header_mappings_dir": "include",
  "preserve_paths": [
    "include/*",
    "include/**/*"
  ],
  "public_header_files": [
    "include/braincloud/*.h",
    "include/braincloud/internal/*.h",
    "include/braincloud/internal/apple/*.h"
  ],
  "source_files": [
    "src/*.{c,cpp}",
    "src/apple/*.{c,cpp,mm}",
    "include/braincloud/*.h",
    "include/braincloud/internal/*.h",
    "include/braincloud/internal/apple/*.h"
  ],
  "exclude_files": [
    "src/DefaultSaveDataHelper.cpp",
    "src/DefaultGUID.cpp",
    "src/DefaultFileUploader.cpp",
    "src/DefaultWebSocket.cpp",
    "src/DefaultPinger.cpp"
  ],
  "xcconfig": {
    "USER_HEADER_SEARCH_PATHS": "\"${SRCROOT}/include/\""
  },
  "libraries": [
    "c++",
    "z"
  ],
  "osx": {
    "frameworks": "LDAP",
    "dependencies": {
      "SocketRocket": [
        "~> 0.5"
      ]
    }
  },
  "dependencies": {
    "SSKeychain": [

    ],
    "BrainCloudJsonCpp": [
      "~>1.1.4"
    ]
  },
  "ios": {
    "dependencies": {
      "SocketRocket": [
        "~> 0.5"
      ]
    }
  },
  "tvos": {
    "dependencies": {
      "SocketRocket": [
        "~> 0.5"
      ]
    }
  }
}
