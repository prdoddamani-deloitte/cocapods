{
  "name": "IMSDKCore",
  "version": "1.0.0",
  "summary": "IM-SDK-Core",
  "description": "OpenIM：基于 Go 实现的即时通讯（IM）项目，iOS版本IM SDK 可以轻松替代第三方IM云服务，打造具备聊天、社交功能的app。",
  "homepage": "https://github.com/OpenIMSDK/Open-IM-SDK-iOS",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "IMSdkCore": "https://github.com/mymirror/IMSdkCore"
  },
  "source": {
    "git": "https://github.com/mymirror/IMSdkCore",
    "tag": "1.0.0"
  },
  "platforms": {
    "ios": "11.0"
  },
  "xcconfig": {
    "VALID_ARCHS": "armv7s arm64 x86_64"
  },
  "pod_target_xcconfig": {
    "ARCHS[sdk=iphonesimulator*]": "$(ARCHS_STANDARD_64_BIT)"
  },
  "vendored_frameworks": "Framework/*.xcframework"
}
