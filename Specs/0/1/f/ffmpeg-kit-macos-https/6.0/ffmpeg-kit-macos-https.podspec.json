{
  "name": "ffmpeg-kit-macos-https",
  "version": "6.0",
  "summary": "FFmpeg Kit macOS Https Shared Framework",
  "description": "Includes FFmpeg with gmp and gnutls libraries enabled.",
  "homepage": "https://github.com/arthenica/ffmpeg-kit",
  "authors": {
    "ARTHENICA": "open-source@arthenica.com"
  },
  "license": {
    "type": "LGPL-3.0",
    "file": "ffmpegkit.xcframework/macos-arm64_x86_64/ffmpegkit.framework/Resources/LICENSE"
  },
  "platforms": {
    "osx": "10.15"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://github.com/arthenica/ffmpeg-kit/releases/download/v6.0/ffmpeg-kit-https-6.0-macos-xcframework.zip"
  },
