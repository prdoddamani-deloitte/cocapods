{
  "name": "libdav1d.swift",
  "version": "1.0.6",
  "summary": "Prebuilt binaries for libwebp",
  "description": "Simple provider for libwebp for iOS with prebuild binaries",
  "homepage": "https://github.com/awxkee/libdav1d.swift",
  "license": {
    "type": "CC0-1.0",
    "file": "LICENSE"
  },
  "authors": {
    "Radzivon Bartoshyk": "radzivon.bartoshyk@proton.me"
  },
  "source": {
    "git": "https://github.com/awxkee/libdav1d.swift.git",
    "tag": "1.0.6"
  },
  "platforms": {
    "ios": "13.0",
    "osx": "11.0",
    "watchos": "6.0",
    "tvos": "11.0"
  },
  "source_files": "Sources/libdav1d.xcframework/ios-arm64/Headers/**/*.h",
  "public_header_files": "Sources/libdav1d.xcframework/ios-arm64/Headers/**/*.h",
  "vendored_frameworks": "Sources/libdav1d.xcframework",
  "preserve_paths": "Sources/libdav1d.xcframework",
  "pod_target_xcconfig": {
    "OTHER_CXXFLAGS": "$(inherited) -std=c++20",
    "HEADER_SEARCH_PATHS": "$(inherited) \"$(PODS_TARGET_SRCROOT)/Sources/libdav1d.xcframework/ios-arm64/Headers\"",
    "EXCLUDED_ARCHS[sdk=watchsimulator*]": "i386"
  },
  "libraries": "c++",
  "requires_arc": true
}
