{
  "name": "YFAdsSDK",
  "version": "5.0.0.17",
  "platforms": {
    "ios": "9.0"
  },
  "requires_arc": true,
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "summary": "iOS YFAdsSDK",
  "description": "欢迎大家使用YFAdsSDK",
  "homepage": "http://www.yifan.com/",
  "authors": {
    "yifan": "576661787@qq.com"
  },
  "source": {
    "git": "https://gitee.com/nanjing-yao-play-club-network/ad-demo.git",
    "tag": "5.0.0.17"
  },
  "user_target_xcconfig": {
    "ENABLE_BITCODE": "NO"
  },
  "pod_target_xcconfig": {
    "ENABLE_BITCODE": "NO"
  },
  "default_subspecs": "Core",
  "static_framework": true,
  "resource_bundles": {
    "YFAdsSDK": [
      "YFAdsSDK/Assets/**/*.*"
    ]
  },
  "xcconfig": {
    "VALID_ARCHS": "armv7 armv7s x86_64 arm64"
  },
  "subspecs": [
    {
      "name": "Core",
      "source_files": "YFAdsSDK/Core/**/*.{h,m}",
      "public_header_files": "YFAdsSDK/Core/**/*.h",
      "frameworks": [
        "UIKit",
        "Foundation",
        "AdSupport"
      ]
    },
    {
      "name": "Adspot",
      "dependencies": {
        "YFAdsSDK/Core": [

        ]
      },
      "source_files": "YFAdsSDK/Adspot/**/*.{h,m}",
      "public_header_files": "YFAdsSDK/Adspot/**/*.h"
    },
    {
      "name": "CSJ",
      "dependencies": {
        "YFAdsSDK/Core": [

        ],
        "YFAdsSDK/Adspot": [

        ],
        "Ads-CN": [

        ]
      },
      "source_files": "YFAdsSDK/Adapter/CSJ/**/*.{h,m}",
      "public_header_files": "YFAdsSDK/Adapter/CSJ/**/*.h",
      "frameworks": [
        "UIKit",
        "MapKit",
        "WebKit",
        "MediaPlayer",
        "CoreLocation",
        "AdSupport",
        "CoreMedia",
        "AVFoundation",
        "CoreTelephony",
        "StoreKit",
        "SystemConfiguration",
        "MobileCoreServices",
        "CoreMotion",
        "Accelerate",
        "AudioToolbox",
        "JavaScriptCore",
        "Security",
        "CoreImage",
        "AudioToolbox",
        "ImageIO",
        "QuartzCore",
        "CoreGraphics",
        "CoreText"
      ],
      "libraries": [
        "c++",
        "resolv",
        "z",
        "sqlite3",
        "bz2",
        "xml2",
        "iconv",
        "c++abi"
      ]
    },
    {
      "name": "GDT",
      "dependencies": {
        "YFAdsSDK/Core": [

        ],
        "YFAdsSDK/Adspot": [

        ],
        "GDTMobSDK": [

        ]
      },
      "source_files": "YFAdsSDK/Adapter/GDT/**/*.{h,m}",
      "public_header_files": "YFAdsSDK/Adapter/GDT/**/*.h",
      "frameworks": [
        "AdSupport",
        "CoreLocation",
        "QuartzCore",
        "SystemConfiguration",
        "CoreTelephony",
        "Security",
        "StoreKit",
        "AVFoundation",
        "WebKit"
      ],
      "libraries": [
        "xml2",
        "z"
      ]
    },
    {
      "name": "KS",
      "dependencies": {
        "YFAdsSDK/Core": [

        ],
        "YFAdsSDK/Adspot": [

        ],
        "KSAdSDK": [

        ]
      },
      "source_files": "YFAdsSDK/Adapter/KS/**/*.{h,m}",
      "public_header_files": "YFAdsSDK/Adapter/KS/**/*.h",
      "frameworks": [
        "Foundation",
        "UIKit",
        "MobileCoreServices",
        "CoreGraphics",
        "Security",
        "SystemConfiguration",
        "CoreTelephony",
        "AdSupport",
        "CoreData",
        "StoreKit",
        "AVFoundation",
        "MediaPlayer",
        "CoreMedia",
        "WebKit",
        "Accelerate",
        "CoreLocation",
        "AVKit",
        "MessageUI",
        "QuickLook",
        "AudioToolBox",
        "AddressBook"
      ],
      "libraries": [
        "z",
        "resolv.9",
        "sqlite3",
        "c++",
        "c++abi"
      ]
    },
    {
      "name": "BD",
      "dependencies": {
        "YFAdsSDK/Core": [

        ],
        "YFAdsSDK/Adspot": [

        ],
        "BaiduMobAdSDK": [

        ]
      },
      "source_files": "YFAdsSDK/Adapter/BD/**/*.{h,m}",
      "frameworks": [
        "CoreLocation",
        "SystemConfiguration",
        "CoreGraphics",
        "CoreMotion",
        "CoreTelephony",
        "AdSupport",
        "SystemConfiguration",
        "QuartzCore",
        "WebKit",
        "MessageUI",
        "SafariServices",
        "AVFoundation",
        "EventKit",
        "QuartzCore",
        "CoreMedia",
        "StoreKit"
      ],
      "libraries": "c++",
      "weak_frameworks": "WebKit",
      "public_header_files": "YFAdsSDK/Adapter/BD/**/*.h"
    }
  ]
}
